{
  "name": "visualeyes",
  "description": "Generate attention heatmaps in seconds",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/loceye/sketch-visualeyes.git"
  },
  "author": "konsalex & dmraptis <konsalexee@gmail.com> <dimitrisraptis96@gmail.com>",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Visualeyes",
    "manifest": "src/manifest.json",
    "main": "visualeyes.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "publish": "skpm publish",
    "postinstall": "npm run build && skpm-link && npm run"
  },
  "devDependencies": {
    "@skpm/builder": "0.7.0"
  },
  "dependencies": {
    "sketch-polyfill-fetch": "^0.4.5"
  }
}
