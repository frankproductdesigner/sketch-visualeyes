{
  "name": "asight-heatmaps",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Asight Heatmaps",
    "manifest": "src/manifest.json",
    "main": "asight-heatmaps.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "0.7.0"
  },
  "author": "konsalex & dmraptis <konsalexee@gmail.com> <dimitrisraptis96@gmail.com>",
  "dependencies": {
    "sketch-polyfill-fetch": "^0.4.5"
  }
}
